{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/*", "src/preload/*"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/renderer/src/*"
      ],
      "@renderer/*": [
        "./src/renderer/src/*"
      ]
    },
    "types": [
      "electron-vite/node"
    ]
  }
}
